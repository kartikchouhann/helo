eventbridge = {
  virginia = {
    rules = [
      {
        name = "acm-certificate-deletion-rule"
        event_pattern_vars = {
          source = "acm"
          events = ["DeleteCertificate"]
        }
        targets = [{ type = "sns", name = "vas-devops-events" }]
      },
      {
        name = "iam-rule"
        event_pattern_vars = {
          source = "iam"
          events = [
            "CreateAccessKey",
            "CreateUser",
            "DeleteAccessKey",
            "DeleteGroup",
            "DeleteUser",
            "RemoveUserFromGroup"
          ]
        }
        targets = [{ type = "sns", name = "vas-devops-events" }]
      },
      {
        name = "organizations-rule"
        event_pattern_vars = {
          source = "organizations"
          events = [
            "CreateAccount",
            "CreateOrganization",
            "CreateOrganizationalUnit",
            "CreatePolicy",
            "DeleteOrganization",
            "DeleteOrganizationalUnit",
            "DeletePolicy",
            "DetachPolicy",
            "DisableAWSServiceAccess",
            "InviteAccountToOrganization",
            "LeaveOrganization",
            "MoveAccount",
            "RemoveAccountFromOrganization",
            "UpdateOrganizationalUnit",
            "UpdatePolicy"
          ]
        }
        targets = [{ type = "sns", name = "vas-devops-events" }]
      },
      {
        name = "route53-rule"
        event_pattern_vars = {
