stages:
- stage: Dev
  condition: or(eq(variables['Resources.TriggeringAlias'], 'devpipeline'), eq(variables['Build.Reason'], 'Manual'))
  jobs:
    - job: BuildJob
      steps:
      - script: echo Hello world! 

- stage: Prod
  condition: or(eq(variables['Resources.TriggeringAlias'], 'prodpipeline'), eq(variables['Build.Reason'], 'Manual'))
